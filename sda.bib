@article{saleem2012generic,
  abstract              = {This paper proposes a query abstraction mechanism which allows web/mobile-service administrator to formulate a skeleton of a sequence of SQL queries by parameterizing holes, later being filled by end-users. The mechanism is generic as the administrator can use it to register multiple services, and expandable as the existing service can be specialized, to automatically generate new kinds of personalized services. An end-user\textquotesingle s input, when given in its entirety, initiates the automatic generation of appropriate SQL queries suitable for the user\textquotesingle s requested service. A personalized service can be devised by designating the enduser\textquotesingle s input parameters into static or dynamic. When static input arguments are given, a specialized skeleton service with respect to the given input is created. The mechanism is implemented to be used in systems for web/mobile-based information and transaction services.},
  author                = {Muhammad Saleem and Iqbal Qasim and Ata ur Rehman},
  bdsk-url-1            = {http://publicationslist.org/data/msaleem/ref-5/JIT\%20Final.pdf},
