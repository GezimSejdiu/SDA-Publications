  keywords              = {2011 group_aksw event_eswc tramp frischmuth ermilov auer arndt seebiproject_MSSW seebiproject_SemanticPingback seebiproject_FederatedSocialWeb sys:relevantFor:infai sys:relevantFor:bis sys:relevantFor:lod2 lod2page peer-reviewed es},
  owner                 = {seebi},
  timestamp             = {2010.12.20},
  title                 = {{W}eaving a {D}istributed, {S}emantic {S}ocial {N}etwork for {M}obile {U}sers},
  url                   = {http://svn.aksw.org/papers/2011/ESWC_MobileSocialSemanticWeb/public.pdf},
  year                  = {2011}
}

