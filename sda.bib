Josiane Xavier Parreira},
  keywords      = {71-4882 168-1466 65-9656 mex simba 2015 sys:relevantFor:infai sys:relevantFor:bis aligned esteves baron usbeck group_aksw lehmann mole soru neto ackermann mack moussallem MOLE aligned-project},
  pages         = {169--176},
  publisher     = {ACM},
  timestamp     = {Wed, 30 Jan 2019 00:00:00 +0100},
  title         = {M{E}X vocabulary: a lightweight interchange format for machine learning
experiments},
  url           = {http://svn.aksw.org/papers/2015/SEMANTICS_MEX/public.pdf},
  year          = {2015}
}

