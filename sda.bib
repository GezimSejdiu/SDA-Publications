  keywords      = {71-4882 20-7514 143-9337 145-1160 2015 group_aksw group_mole mole stadler lehmann sherif sys:relevantFor:geoknow geoknow peer-reviewed MOLE westphal},
  publisher     = {CEUR-WS.org},
  series        = {CEUR Workshop Proceedings},
  timestamp     = {Wed, 12 Feb 2020 16:44:23 +0100},
  title         = {{S}implified {R}DB2{R}DF {M}apping},
